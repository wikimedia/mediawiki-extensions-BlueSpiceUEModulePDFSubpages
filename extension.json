{
	"name": "BlueSpiceUEModulePDFSubpages",
	"version": "4.0.0",
	"license-name": "GPL-3.0-only",
	"url": "https://en.wiki.bluespice.com/wiki/Reference:UEModulePDFSubpages",
	"author": [
		"Robert Vogel",
		"Tobias Weichart"
	],
	"descriptionmsg": "bs-uemodulepdfsubpages-extension-description",
	"requires": {
		"MediaWiki": ">= 1.35.0",
		"extensions": {
			"BlueSpiceFoundation": ">= 4.0",
			"BlueSpiceUniversalExport": ">= 4.0"
		}
	},
	"type": "bluespice",
	"attributes": {
		"BlueSpiceFoundation": {
			"Extensions": {
				"BlueSpiceUEModulePDFSubpages": {
					"className": "UEModulePDFSubpages",
					"extPath": "/BlueSpiceUEModulePDFSubpages",
					"package": "BlueSpice Pro",
					"status": "stable"
				}
			},
			"PermissionRegistry": {
				"uemodulepdfsubpages-export": {
					"type": "namespace",
					"roles": [
						"reader"
					]
				}
			}
		}
	},
	"AutoloadNamespaces": {
		"BlueSpice\\UEModulePDFSubpages\\": "src/"
	},
	"GroupPermissions": {
		"*": {
			"uemodulepdfsubpages-export": true
		}
	},
	"AvailableRights": [
		"uemodulepdfsubpages-export"
	],
	"MessagesDirs": {
		"BlueSpiceUEModulePDFSubpages": [
			"i18n"
		]
	},
	"AutoloadClasses": {
		"UEModulePDFSubpages": "UEModulePDFSubpages.class.php"
	},
	"Hooks": {
		"ChameleonSkinTemplateOutputPageBeforeExec": "\\BlueSpice\\UEModulePDFSubpages\\Hook\\ChameleonSkinTemplateOutputPageBeforeExec\\AddWidget::callback",
		"BSUEModulePDFBeforeAddingContent": "\\BlueSpice\\UEModulePDFSubpages\\Hook\\BSUEModulePDFBeforeAddingContent\\AddSubPages::callback"
	},
	"load_composer_autoloader": true,
	"manifest_version": 2
}
